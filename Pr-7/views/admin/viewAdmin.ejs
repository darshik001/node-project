<%- include('../header')%>
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- Page wrapper  -->
    <!-- ============================================================== -->
    <div class="page-wrapper">
        <!-- ============================================================== -->
        <!-- Bread crumb and right sidebar toggle -->
        <!-- ============================================================== -->
        <div class="page-breadcrumb">
            <div class="row">
                <div class="col-12 d-flex no-block align-items-center">
                    <h4 class="page-title">admin</h4>
                    <div class="ml-auto text-right">
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="/">Home</a></li>
                                <li class="breadcrumb-item active" aria-current="page">admin</li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- End Bread crumb and right sidebar toggle -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Container fluid  -->
        <!-- ============================================================== -->
        <div class="container-fluid">
            <!-- ============================================================== -->
            <!-- Start Page Content -->
            <!-- ============================================================== -->
            <div class="row">
                <div class="col-12">

                    <div class="card">
                        <div class="card-body">

                            <div class="row justify-content-around align-items-center mb-5">
                                <div class="col-4">
                                    <h5 class="card-title">Admins details </h5>

                                </div>
                                <div class="col-4">
                                    <form action="">
                                        <div class="input-group">
                                            <input type="search" class="form-control" name="search">
                                            <button class="btn btn-outline-primary">search</button>


                                        </div>
                                    </form>
                                </div>
                            </div>

                            <div class="table-responsive">
                                <table id="zero_config" class="table table-striped table-bordered">
                                    <thead>
                                        <tr>
                                            <th>Profile</th>
                                            <th>
                                                Name
                                            </th>
                                            <th>Gender</th>
                                            <th>Contact</th>
                                            <th>Email</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>

                                        <% admins.forEach((admin,i)=> {%>
                                            <tr class="align-middle">
                                                <td class="align-middle"><img src="<%=admin.profileImage%>"
                                                        class="rounded rounded-circle" width="50" height="50"
                                                        alt="<%=admin.firstname%>"></td>
                                                <td class="align-middle">
                                                    <%=admin.firstname%>
                                                        <%=admin.lastname%>
                                                </td>
                                                <td class="align-middle">
                                                    <%=admin.gender%>
                                                </td>
                                                <td class="align-middle">
                                                    <%=admin.contactNo%>
                                                </td>
                                                <td class="align-middle">
                                                    <%=admin.email%>
                                                </td>
                                                <td class="align-middle">
                                                    <a href="/admin/edit-admin/<%=admin.id%>"
                                                        class="btn btn-cyan btn-sm">Edit</a>
                                                    <a href="/admin/delete-admin/<%=admin.id%>"
                                                        class="btn btn-danger btn-sm">delete</a>
                                                </td>
                                            </tr>
                                            <%})%>

                                    </tbody>
                                </table>
                            </div>

                        </div>
                    </div>
                </div>
            </div>

            <!-- ============================================================== -->
            <!-- End PAge Content -->
            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- Right sidebar -->
            <!-- ============================================================== -->
            <!-- .right-sidebar -->
            <!-- ============================================================== -->
            <!-- End Right sidebar -->
            <!-- ============================================================== -->
        </div>
        <!-- ============================================================== -->
        <!-- End Container fluid  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- footer -->
        <%- include('../footer')%>